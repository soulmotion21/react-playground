    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "dayjs": "^1.10.7",
    "dompurify": "^2.3.4",
    "echarts": "^5.1.1",
    "emotion": "^11.0.0",
    "emotion-ts-plugin": "^1.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
