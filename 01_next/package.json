{
  "name": "react-dashboard",
  "version": "1.0.0",
  "description": "react-dashboard project",
  "main": "index.js",
  "engines": {
    "node": ">=12.16.1"
  },
  "scripts": {
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,ts,tsx,css,scss,yml}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "cleanup": "rimraf build/resources/main/static/",
    "start": "npm run webpack:dev",
    "webpack:dev": "npm run webpack serve -- --config webpack/webpack.dev.js",
    "webpack:build:main": "npm run webpack -- --config webpack/webpack.dev.js --stats=minimal",
    "webpack:build": "npm run cleanup && npm run webpack:build:main",
    "webpack:prod:main": "npm run webpack -- --config webpack/webpack.prod.js",
    "webpack:prod": "npm run cleanup && npm run webpack:prod:main",
    "webpack-dev-server": "node --max-old-space-size=8192 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js",
    "test": "npm run lint && npm run jest",
    "sass": "sass --watch ./src/main/webapp/app/asset/style",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config src/test/javascript/jest.config.js"
  },
  "devDependencies": {
    "@easylogic/react-summernote": "^0.0.9",
    "@emotion/eslint-plugin": "^11.5.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@formatjs/ts-transformer": "^3.6.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/dompurify": "^2.3.2",
    "@types/echarts": "^4.9.7",
    "@types/jest": "^26.0.23",
    "@types/jquery": "^3.5.10",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
